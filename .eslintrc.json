{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard"],
  "ignorePatterns": ["**/.netlify/**/*"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "comma-dangle": "off"
  }
}
